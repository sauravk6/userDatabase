@import '_mixins'

global-width=1100px;

.fl-l{
	float: left;
}
.fl-r{
	float: right;
}
.dib{
	display: inline-block;
	*display:inline;
	zoom:1;
	font-size: 16px;
	font-size: 1rem;
}
.overflow-hidden {
	overflow: hidden;
}
.fLW{
	font-family: "LobsterWeb","Open Sans",Arial, Helvetica, sans-serif;
}
.fOS{
	font-family: "Open Sans", Arial, Helvetica, sans-serif;
}
.fOSC{
	font-family: "Open Sans Condensed",Arial, Helvetica,sans-serif;
}

.va-t{
	vertical-align: top;
}

.va-m{
	vertical-align: middle;
}

.va-b{
	vertical-align: bottom;
}

.va-bl{
	vertical-align: baseline;
}

html, body {
	height: 100%;
}

.modal-open{
	overflow:hidden
	#header{
		z-index 0
	}
}

#global-wrapper{
	min-height: 100%;
	height: auto !important;
	height: 100%;
	background: white;
	margin: 0 auto -4em;
}

.hideHeader{
	top: -80px !important;
}

.container{
	max-width: 980px;
	margin: 0 auto;
}

#header{
	background: white;
	position: relative;
	box-shadow:0 0 2px 1px #666;
	left: 0px;
	right: 0px;
	
	z-index: 10;
}

#header-wrap{
	background: white;
	position: relative;
	overflow hidden
}

#footer, #push {
	height: 4em;
}

#footer{
	min-height: 230px;
	background: #111;
	padding-bottom: 28px;
}

#footer-links{
	margin-top: 40px;
	min-height: 50px;
	text-align: center;
	font-size: 0.8em;
	color: white;
	position: relative;
	a{
		color: white;
		display: inline-block;
		&:hover{
			text-decoration: underline;
		}
	}

}
.lastUnit{
	font-size: 13px;
}
.unit_each{
	color: #737373;
	font-weight: bold;
	font-size: 13px;
}
.ftr-link-header{
	margin: 0.3em 0;
	font-weight: lighter;
	font-size: 1.5em;
}
.footer_link_main{
	font-size: .8em;
	background: #fafafa;
	padding: 20px 15px;
	text-align: justify;
}
.ftr-line-links{
	color: #777171;
	padding: 2px 1px;
	font-size: 13px;
	&:hover{
		color: primary-color;
	}
}
.brands{
	margin-bottom: 2px; 
}
#media{
	position: relative;
	text-align: center;
	min-height: 65px;
	padding-top: 23px;
	span{
		position: relative;
		font-size: 0.55em;
		color: white;
		bottom: -60px;
		font-weight: bold;
	}
}
.social{
	margin: 10px;
	width: 55px;
	height: 55px;
	@extend .bg-sprite;
	display: inline-block;
	opacity: 0.8;
	transition opacity 0.5s ;
}

.fb{
	background-position: -9px -445px;
	&:hover{
		opacity: 1;
	}
}
.tw{
	background-position: -75px -445px;
	&:hover{
		opacity: 1;
	}
}
.gp{
	background-position: -141px -445px;
	&:hover{
		opacity: 1;
	}
}

#email{
	margin-top: 16px;
	padding: 8px 10px;
	color: #ccc;
	text-align: left;
	&:focus{
		color: #666;
	}
	border-radius 3px
	border: 0;
}

@media (max-width: 480px){ // Mobile optimized.   

	.social{
		opacity: 1;
	}
}

#logo{
	margin: .5em;
	+bp(max,670){
		di(middle);
		margin-left: 2.7em;
	}
	
	+bp(min,670){
		float: left;
		margin-left: 2.7em;
	}
	+bp(min,pad){
		margin-left:.5em
	}
	img{
		height: 3.3em;
		margin-top: -.25em;
	}
}

.search-div{
	padding: .5em;
	+bp(min,670){
		margin: 0 7.5em 0 16em;
		padding: .7em 0.3em;
	}
	+bp(min,pad){
		margin: 0 7.5em 0 14em;
	}

}
#categ-show-btn{
	position: relative;
	padding:0px 10px 5px;
	text-align:center;
	+bp(min, pad){
		display: none
	}
	&.active +.nav-item{
		height:auto;
	}
	a{
		margin-left: 1em;
		font-size: 1.1em;
		text-transform: uppercase;
		color: #666;
	}
}
.menu-toggle{
	position: absolute;
	width: 20px;
	border-bottom:3px solid #888;
	top: 11px;
	margin-left: -10px;
	&:before,&:after{
		content:"";
		display: block;
		top:-8px;
		position: absolute;        
		width: 20px;
		border-bottom:3px solid #888;
	}
	&:after{
		top:8px;
	}
}
#sub-heads{
	position: absolute;
	margin: .5em;
	top: 0;
	right: 0;
	a{
		di(middle);
		@extend .bg-sprite;
		height: 3.1em;
		width: 3.1em;
	}
	.subscribe{
		background-position: -107px -129px;
	}
	.add-ons{
		background-position: 0px -128px;
	}
}
#search-contents{
	position: relative;
	text-align: left;
	margin-right:4.5em;
}
.search-wrap{
	position:relative;
	input[type="text"]{
		width: 100%;
		padding: .4em 1em;
		font-size: .938em;
		height: 2.7em;
		border-radius .33em
		border: 1px solid #ccc;
		color:#333;
		z-index:3;
		&:focus{
			border:1px solid #0db2db;
		}
	}
	button{
		float: right;
		cursor: pointer;
		width: 4em;
		height: 2.563em;
		margin:-2.5em 0 0 .125em;
		border-radius: .25em;
		background: #0db2db;
		background: linear-gradient(to bottom,  #0db2db 0%,#0b99c4 100%);

		border: 0;
		right: 0;
		top: 0;
		z-index: 3;
		img{
			margin-top: -1px;
		}
		&:hover{
			background: #0fc1ed; /* Old browsers */
			background: linear-gradient(to bottom,  #0fc1ed 0%,#10add4 100%);
		}
		&:active{
			background: #0c86a4; /* Old browsers */
			background: linear-gradient(to bottom,  #0c86a4 0%,#0c9ec2 100%);
		}
	}
}

