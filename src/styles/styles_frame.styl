/***************breadcrumb**********/
#breadcrumb {
	max-width:global-width;
	margin: auto;
	font-size: .8em;
	padding: .625em 2em;
	ul {
		li {
			float: left;
			text-transform: uppercase;
			margin-top: 2px;
			position: relative;
			a {
				margin: 0 5px;
				color: #0d99aa;
				&:hover{
					text-decoration:underline;
					color: #0db2db;
				}
			}
			&.first{
				background: #0d99aa;
				font-size: 16px;
				margin: -0.1em 1.1em 0 0;
				border-radius: 100%;
				a
				{
					@extend .bg-sprite;
					background-position: -97px -395px;
					width: 1.436em;
					height: 2.058em;
					padding: 0.5em 0.05em;
					position: relative;
					display: block;

				}
				&:after{
					position: absolute;
					top: 0em;
					right: -0.8em;
				}
			}
			&:after{
				content: "\00bb";
				color: #777;
				font-size: 1.2em;
				vertical-align: bottom;
			}
			&.current{
				padding: 0 5px;
				&:after{
					color:transparent;
				}
			}
		}
	}
}
