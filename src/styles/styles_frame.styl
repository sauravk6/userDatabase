	span {
		/* line in the center */
		position: absolute;
		top: 50%;
		right: 10px;
		margin-top: -2px;
		transition:background .3s;
	}

	span:before, span:after {
		/* other 2 lines */
		content: '';
		right: 0;
		/* Force Hardware Acceleration in WebKit */
		transform translateZ(0)
		backface-visibility: hidden;
		transition:top 0.3s, bottom 0.3s, transform 0.3s;
	}

	span:before {
		/* menu icon top line */
		top: -6px;
	}

	span:after {
		/* menu icon bottom line */
		bottom: -6px;
