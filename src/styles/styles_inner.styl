/***************countdown***********/
.is-countdown {
	padding 0 0 5px 2%
}
.countdown-rtl {
	direction: rtl;
}
.countdown-holding span {
	color: #888;
}
.countdown-row {
	clear: both;
	width: 100%;
	padding: 0px 2px;
	text-align: center;
}
.countdown-show1 .countdown-section {
	width: 98%;
}
.countdown-show2 .countdown-section {
	width: 48%;
}
.countdown-show3 .countdown-section {
	width: 32.5%;
}
.countdown-show4 .countdown-section {
	width: 24.5%;
}
.countdown-show5 .countdown-section {
	width: 19.5%;
}
.countdown-show6 .countdown-section {
	width: 16.25%;
}
.countdown-show7 .countdown-section {
	width: 14%;
}
.countdown-section 
	display: block;
	float: left;
	font-size: 63%;
	text-align: center;
	padding 3px 0 7px
	margin-right:-1px
	color: #CB4B4B;
	position relative
	&:after
		content:":";
		position: absolute;
		top:10px;
		left:-1px
	&:first-child:after
		content:none

.countdown-amount {
    font-size: 200%;
}
.countdown-period {
    display: block;
    color: #555;
}

