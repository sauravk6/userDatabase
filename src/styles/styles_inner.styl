		label{
			cursor: pointer;
			display: block;
			input{
				vertical-align: middle;
				margin-right: .3em;
			}
		}
	}



	li{
		font-size: .9em;
		color: #555;
		
		>ul{
			display: none;
			padding: 0 0 0 .7em;
			padding: 0 0 0 .7rem;

		}
		
		&{
			a:hover{
				color: white;
			background: #50CCee;
			}
		}
		a{
			color:  #666;
			padding: 0.5em 0.5em 0.5em 1.3em;
			display: block;
			transition none;			
			margin: -0.2em 0;
			position relative
			font-size: .9em;
		}
	}	
	.it-count{
		margin-left: .5em;
		color: #999;
	}
}
.more-btn-wrapper
	text-align center
	

.btn-see-more
	text-align: center;
	margin: 0.5em auto 3em;	
	background: primary-color
	color white!important
	border 1px solid darken(@background,10%)
	&:hover
		background lighten(@background,20%)
	&:active
		background darken(@background,10%)
	&:after{
		display: none
	}
/****************drop down********/
.side-dropdown
	position: relative;
	margin:3px 0 0 4em;
	padding: 0.4em 2em 0.4em .625em;
	background: #fff;
	border-radius 3px;
	border: 1px solid #d5d5d5;
	cursor: pointer;
	outline: none;
	color: #808080;
	font-size .9em
	@media (max-width:50em)
			font-size 1em
	&:before
		content:"Sort By:"	
		left: -4em;
		position absolute
		color: #555;

	@media (min-width:50em)
		width: 15em;
		position: absolute;
		right: 0;
		top: -0.1em;

	&:hover
		box-shadow 0px 1px 6px 0px #bbb;

	&.active
		background: #0db2db;
		box-shadow none;
		border-bottom: none;
		color: white;
		.dropdown
			visibility:visible;
			pointer-events: auto;


	&:after
		content: "";
		width: 0;
		height: 0;
		position: absolute;
		right: 15px;
		top: 50%;
		margin-top: -3px;
		border-width: 5px 5px 0 5px;
		border-style: solid;
		border-color: #999 transparent;

	&.active:after
		border-color: #ffffff transparent;

	.dropdown  
		position: absolute;
		z-index: 10;
		top: 140%;
		left: 0;
		right: 0;
		background: white;
		border-radius: inherit;
		border: 1px solid #d5d5d5;
		box-shadow: 0 0 5px #d5d5d5;
		font-weight: normal;
		list-style: none;
		visibility:hidden;
		pointer-events: none;

		&:after
			content: "";
			width: 0;
			height: 0;
			position: absolute;
			bottom: 100%;
			right: 15px;
			border-width: 0 7px 7px 7px;
			border-style: solid;
			border-color: #fff transparent;	

		&:before
			content: "";
			width: 0;
			height: 0;
			position: absolute;
			bottom: 100%;
			right: 12px;
			border-width: 0 10px 10px 10px;
			border-style: solid;
			border-color: #d5d5d5 transparent;	

		li
			a
				display: block;
				padding: 0.6em;
				text-decoration: none;
				color: #808080;
				border-bottom: 1px solid #e6e8ea;

			&:first-of-type a
				border-radius: 3px 3px 0 0;

			&:last-of-type a
				border-radius: 0 0 3px 3px;
				border: none;

			&:hover a
				color: white!important;
				background: #0db2db;
/*********************buttons**********/

